def addIntVector (v1,v2):
    # Not length-safe
    return [v1[i] + v2[i] for i in range (len(v1))]

def subtractIntVector (v1,v2):
    # Not length-safe
    return [v1[i] - v2[i] for i in range (len(v1))]

def sameIntVector (v1,v2):
    for i in range(len(v1)):
        if not v1[i] == v2[i]:
            return False
    return True


moves=[subtractIntVector (b[0],b[1]) for b in baseEdges]

def isApplicable (i,m):
    # Test if a move is applicable
    for k in range (len(i)):
        if i[k] < m[k]:
            return False
    return True

# First try: Create a graph
# This function describes adjacencies
def are_adjacent (i,j):
    for m in moves:
        if sameIntVector (j, addIntVector(i,m)): return True
        if sameIntVector (j, subtractIntVector(i,m)): return True
    return False

