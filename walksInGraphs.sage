# Compute all monomials of degree d in the ring R
def degreeMonomials (R, d):
    exponents = IntegerVectors(d,R.ngens()).list()
    return [prod ([R.gen(i)^e[i] for i in range (R.ngens())])
            for e in exponents]
    

